    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "btoa": "1.1.2",
    "d3-hierarchy": "^1.1.8",
    "d3v4": "^4.2.2",
    "jwt-simple": "^0.5.5",
    "mongoose": "^5.3.6",
    "password-hash": "^1.2.2",
    "prop-types": "^15.6.2",
    "react": "16.4.x",
    "react-dom": "16.4.x",
    "react-motion": "^0.5.2",
    "react-redux": "5.0.6",
    "react-router": "3.2",
    "react-spring": "^7.2.8",
